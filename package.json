{
  "name": "bloglist",
  "version": "1.0.0",
  "description": "App that allows users to save information about interesting blogs",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "lint": "eslint .",
    "format": "prettier . --write",
    "start": "cross-env NODE_ENV=production node index.js",
    "start:test": "cross-env NODE_ENV=test node index.js",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand --forceExit",
    "test:dev": "cross-env NODE_ENV=test jest --watch --runInBand --forceExit",
    "test:cov": "cross-env NODE_ENV=test jest --runInBand --forceExit --collectCoverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-rikkokiri:Rikkokiri/FullStackOpen-bloglist.git"
  },
  "author": "O. Rajala",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.11.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3"
  }
}
